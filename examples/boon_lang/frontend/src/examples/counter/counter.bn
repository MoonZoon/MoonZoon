document: Document/new(root: Element/stripe(
    element: []
    direction: Row
    style: []
    items: LIST {
        Element/container(
            element: []
            style: []
            child: 'I am a counter: '
        )
        counter
    }
))

counter: 
    LATEST {
        0
        Duration[seconds: 50] |> Timer/interval() |> THEN { 1 } 
    } 
    |> Math/sum()
